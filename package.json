{
  "name": "polymer-cli",
  "version": "5.0.11",
  "private": true,
  "scripts": {
    "build-storybook": "build-storybook -c .storybook -s ./static",
    "start": "webpack-dev-server",
    "storybook": "start-storybook -p 9001 -c .storybook -s ./static && gulp"
  },
  "dependencies": {
    "@babel/core": "^7.4.4",
    "@open-wc/demoing-storybook": "^0.2.2",
    "@polymer/polymer": "^2.6.0",
    "@storybook/addon-a11y": "^5.0.11",
    "@storybook/addon-actions": "5.0.11",
    "@storybook/addon-backgrounds": "5.0.11",
    "@storybook/addon-knobs": "5.0.11",
    "@storybook/addon-links": "5.0.11",
    "@storybook/addon-notes": "5.0.11",
    "@storybook/addon-options": "5.0.11",
    "@storybook/addon-storysource": "5.0.11",
    "@storybook/addon-viewport": "5.0.11",
    "@storybook/polymer": "5.0.11",
    "@webcomponents/webcomponentsjs": "^1.2.0",
    "babel-loader": "^8.0.0",
    "global": "^4.3.2",
    "lit-element": "^2.1.0",
    "lit-html": "^1.0.0",
    "polymer-webpack-loader": "^2.0.3",
    "storybook-dark-mode": "^0.1.3",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.3.1"
  },
  "devDependencies": {
    "copy-webpack-plugin": "^4.6.0",
    "del": "^4.1.1",
    "eslint": "^5.16.0",
    "eslint-config-google": "^0.13.0",
    "eslint-plugin-wc": "^1.1.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^6.1.0",
    "gulp-cssimport": "^7.0.0",
    "gulp-csso": "^3.0.1",
    "gulp-if": "^2.0.2",
    "gulp-postcss": "^8.0.0",
    "gulp-rename": "^1.4.0",
    "gulp-sourcemaps": "^2.6.5",
    "html-webpack-plugin": "^4.0.0-beta.2",
    "husky": "^2.3.0",
    "lint-staged": "^8.1.6",
    "postcss-color-mod-function": "^3.0.3",
    "postcss-custom-properties": "^8.0.10",
    "prettier": "^1.17.1",
    "webpack-dev-server": "^3.1.14"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "*.{js,json,md}": [
        "eslint --fix",
        "prettier --write",
        "git add"
      ]
    },
    "ignore": [
      "static/**/*,",
      "package.json",
      "package-lock.json"
    ]
  }
}